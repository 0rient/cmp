<skin name="VPlayer" version="1.1" author="CenFun" email="cenfun@gmail.com" url="http://bbs.cenfun.com/" readme="" show_tips="0">
	<list xywh="0C, 0, 60P, 52P" src="list.png" lock="1" display="0" group="A" shadow="3,45,#000000,0.5,4,4,1,3">
		<tree xywh="5, 1, 5B, 15B" align="left" font="Arial" size="12" color="#666666, #ffffff" />
		<scrollbar thumb_src="scrollbar_thumb.png" button_src="scrollbar_button.png" />
		<search xywh="5, 6R, 150, 18" align="left" font="Arial" size="12" color="#555555, #999999" borderColor="#444444" backgroundColor="#282828" value="请输入关键字" />
		<bt_close xywh="5R, 5R, 60, 18" src="bt_close.png" />
	</list>
	<console xywh="0, 0, 100P, 100P" src="vplayer.swf" lock="1" display="1" group="A">
		<bt_play xywh="10, 10R, 40, 30" src="bt_play.png" />
		<bt_mute xywh="50R, 10R, 30, 30" src="bt_mute.png" />
		<volume xywh="-23R, 30R, 100, 24" track_src="track_volume.png" rotation="-90" />
		<progress xywh="63, 13R, 125B, 24" track_src="track_progress.png" />
		<bt_fullscreen xywh="10R, 10R, 30, 30" src="bt_fullscreen.png" tips="" />
		<time xywh="90R, 18R, 98, 16" align="center" font="Verdana" size="11" color="#cccccc" borderColor=""  />
		<bt_list xywh="0C, 0, 80, 20" src="bt_list.png" />
	</console>
	<media xywh="0, 0, 100P, 100P" src="bg.swf" lock="1" display="1" mask="mask.swf" group="A">
		<video xywh="1, 1, 1B, 1B" />
	</media>
</skin>